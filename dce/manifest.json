{
  "name": "CDE",
  "version": "1.0",
  "description": "Alt + X (default) for alert how many DOM node element there is on a webpage.",
  "manifest_version": 2,
  "permissions": [
    "activeTab","tabs","identity","*://*/*"
  ],
  "background": {
    "scripts": ["count.js"],
    "persistent": false
  },
  "commands": {
    "calculate-DOM": {
      "suggested_key": {
        "default": "Alt+X",
        "mac": "Alt+X"
      },
      "description": "Just display the number of DOM elements."
    }
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon.png" 
  }
}